{
  "name": "natural-cycles",
  "version": "1.0.0",
  "description": "Backend Challenge. Firebase require node >=8.13.0",
  "engines": {
    "node": ">=8.13.0"
  },
  "engineStrict": true,
  "main": "dist/index.js",
  "scripts": {
    "start": "NODE_ENV=production node .",
    "build": "tsc --outDir dist",
    "dev": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "_dev": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-ts": "tsc -w --outDir dist",
    "watch-node": "NODE_ENV=development nodemon .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "express",
    "typescript",
    "prettier",
    "session",
    "CRUD",
    "firebase",
    "realtime database"
  ],
  "author": "Baptiste Clarey Sj√∂strand",
  "license": "GPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "https://github.com/baptistecs/Natural-Cycles.git"
  },
  "dependencies": {
    "blake2b": "^2.1.3",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "firebase-admin": "^8.5.0",
    "firestore-store": "^2.0.1",
    "method-override": "^3.0.0"
  },
  "devDependencies": {
    "@types/concurrently": "^4.1.0",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/express-session": "^1.15.14",
    "@types/method-override": "0.0.31",
    "@types/node": "^12.7.5",
    "concurrently": "^5.0.0",
    "dotenv": "^8.2.0",
    "nodemon": "^2.0.1",
    "prettier": "1.18.2",
    "typescript": "^3.6.3"
  }
}
